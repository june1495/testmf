{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"mf-crud": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss",
					"standalone": false
				},
				"@schematics/angular:directive": {
					"standalone": false
				},
				"@schematics/angular:pipe": {
					"standalone": false
				}
			},
			"root": "projects/mf-crud",
			"sourceRoot": "projects/mf-crud/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/mf-crud",
						"index": "projects/mf-crud/src/index.html",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/mf-crud/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/mf-crud/public",
								"output": "."
							}
						],
						"styles": [
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
							"projects/mf-crud/src/styles.scss"
						],
						"scripts": [],
						"main": "projects/mf-crud/src/main.ts",
						"extraWebpackConfig": "projects/mf-crud/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kB",
									"maximumError": "4kB"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/mf-crud/webpack.prod.config.js"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "mf-crud:build:production",
							"extraWebpackConfig": "projects/mf-crud/webpack.prod.config.js"
						},
						"development": {
							"buildTarget": "mf-crud:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4201,
						"publicHost": "http://localhost:4201",
						"extraWebpackConfig": "projects/mf-crud/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/mf-crud/tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/mf-crud/public"
							}
						],
						"styles": [
							"projects/mf-crud/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		},
		"mf-shell": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss",
					"standalone": false
				},
				"@schematics/angular:directive": {
					"standalone": false
				},
				"@schematics/angular:pipe": {
					"standalone": false
				}
			},
			"root": "projects/mf-shell",
			"sourceRoot": "projects/mf-shell/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/mf-shell",
						"index": "projects/mf-shell/src/index.html",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/mf-shell/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/mf-shell/public",
								"output": "."
							}
						],
						"styles": [
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
							"projects/mf-shell/src/styles.scss"

						],
						"scripts": [],
						"main": "projects/mf-shell/src/main.ts",
						"extraWebpackConfig": "projects/mf-shell/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kB",
									"maximumError": "4kB"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/mf-shell/webpack.prod.config.js"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "mf-shell:build:production",
							"extraWebpackConfig": "projects/mf-shell/webpack.prod.config.js"
						},
						"development": {
							"buildTarget": "mf-shell:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4200,
						"publicHost": "http://localhost:4200",
						"extraWebpackConfig": "projects/mf-shell/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/mf-shell/tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/mf-shell/public"
							}
						],
						"styles": [
							"projects/mf-shell/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
    },
    "commons-lib": {
      "projectType": "library",
      "root": "projects/commons-lib",
      "sourceRoot": "projects/commons-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/commons-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/commons-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/commons-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/commons-lib/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
	},
	"cli": {
		"analytics": false
	}
}
